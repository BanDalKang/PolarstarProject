<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.polarstarproject">

    <!-- 인터넷 사용 권한 허용-->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/polaris_roughly"
        android:label="@string/app_name"
        android:roundIcon="@drawable/polaris_roughly"
        android:supportsRtl="true"
        android:theme="@style/Theme.PolarstarProject">
        <activity
            android:name=".DisabledRegisterActivity"
            android:exported="false"
            android:parentActivityName=".UserSelectActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".UserSelectActivity"
            android:exported="true">
        </activity>

    </application>

</manifest>

